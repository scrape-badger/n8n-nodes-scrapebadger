{
  "name": "n8n-nodes-scrapebadger",
  "version": "0.1.0",
  "description": "n8n community node for ScrapeBadger - Twitter/X scraping API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "twitter",
    "x",
    "scraping",
    "scrapebadger",
    "social-media"
  ],
  "license": "MIT",
  "homepage": "https://scrapebadger.com",
  "author": {
    "name": "ScrapeBadger",
    "email": "support@scrapebadger.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scrape-badger/n8n-nodes-scrapebadger.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "lintcheck": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ScrapeBadgerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ScrapeBadger/ScrapeBadger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
